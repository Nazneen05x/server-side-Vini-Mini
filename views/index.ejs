<%- include('./partials/head') %>

  <main>

    <h1 class="title">Alle producten</h1>
    
    <div class="content">

    <% categories.forEach(category => { %>
          <article class="content_producten"> 
              <h1><%= category.titel %></h1>
              <img class="article-img" src="/<%=category.titel%>.png">
              <a href="<%= category.titel %>"> Bekijk producten</a>
          </article>
      </a>
  <% }); %>

</div>

    
  </main>

  <%- include('./partials/foot') %>